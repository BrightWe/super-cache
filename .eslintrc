{
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "extends": [
      "airbnb-base",
      "eslint:recommended"
    ],
    "plugins": [
      "import"
    ],
    "rules": {
      "comma-dangle": ["error", "always-multiline"],
      "semi": ["error"],
      "no-console": ["warn"]
    },
    "env": {
      "node": true
    }
  }