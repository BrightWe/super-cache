{
  "name": "@jerryc/super-cache",
  "version": "0.1.1",
  "description": "An extended cache library.",
  "main": "dist/super-cache.js",
  "scripts": {
    "test": "ava",
    "test:coverage": "nyc --reporter=html ava",
    "build-pro": "babel src -d dist",
    "build-watch": "babel src --watch -d dist",
    "code-check": "eslint src/**",
    "code-fix": "eslint --fix src/**",
    "lint": "git diff --cached --name-only | grep -E '\\.(js|jsx)$' | xargs eslint",
    "commitmsg": "validate-commit-msg",
    "commit": "git cz"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/JerryC8080/super-cache.git"
  },
  "author": "JerryC (huangjerryc@gmail.com)",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "ava": "^1.2.1",
    "babel-eslint": "^9.0.0",
    "commitizen": "^3.0.5",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "nyc": "^14.0.0",
    "pre-commit": "^1.2.2",
    "validate-commit-msg": "^2.14.0"
  },
  "bugs": {
    "url": "https://github.com/JerryC8080/super-cache/issues"
  },
  "homepage": "https://github.com/JerryC8080/super-cache#readme",
  "directories": {
    "test": "test"
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "require": [
      "@babel/register",
      "@babel/polyfill"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
